age_groups <- c("0-9", "10-19", "20-29", "30-39", "40-49", "50-59", "60-69", "70-79", "80+")
cases <- c(10, 15, 20, 30, 40, 50, 60, 70, 80)
person_years <- c(1000, 1500, 2000, 2500, 3000, 3500, 4000, 4500, 5000)
incidence_data <- data.frame(age_groups, cases, person_years)
incidence_data$incidence_rate <- incidence_data$cases / incidence_data$person_years * 1000
standard_population <- c(1000, 2000, 3000, 4000, 5000, 6000, 7000, 8000, 9000)
standardized_rate <- standardize(incidence_data$incidence_rate, standard_population, weights = incidence_data$person_years)
